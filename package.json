{
  "name": "tursakur",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "description": "Türkiye Sağlık Kuruluşları Database - Modern React Frontend with Supabase Backend",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier --write src/",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx vite-bundle-analyzer",
    "clean": "rm -rf dist node_modules/.vite",
    "migrate": "python scripts/load_to_supabase.py",
    "test-db": "python scripts/supabase_client.py",
    "vercel-build": "npm run build",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "test:install": "playwright install",
    "test:codegen": "playwright codegen localhost:3000",
    "test:smoke": "playwright test --project=chromium tests/homepage.spec.js tests/database.spec.js",
    "test:production": "playwright test --project=production",
    "test:ci": "playwright test --reporter=github"
  },
  "dependencies": {
    "@playwright/test": "^1.54.2",
    "@supabase/supabase-js": "^2.53.0",
    "@tanstack/react-query": "^5.84.0",
    "@tanstack/react-query-devtools": "^5.84.0",
    "leaflet": "^1.9.4",
    "prop-types": "^15.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.7.1",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.0.4"
  }
}
